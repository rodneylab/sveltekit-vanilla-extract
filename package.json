{
	"name": "sveltekit-vanilla-extract",
	"version": "0.0.1",
	"license": "BSD-3-Clause",
	"description": "Using vanilla-extract with SvelteKit: how you can make zero-runtime stylesheets with the new TypeScript friendly preprocessor in Svelte.",
	"scripts": {
		"dev": "vite dev",
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin=prettier-plugin-svelte . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin=prettier-plugin-svelte .",
		"prepare": "husky install",
		"prettier:check": "prettier --check --plugin=prettier-plugin-svelte .",
		"svelte-check": "svelte-check --fail-on-hints"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^19.8.0",
		"@eslint/js": "^9.22.0",
		"@fontsource/source-sans-pro": "^5.2.5",
		"@fontsource/source-serif-pro": "^5.2.5",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "2.19.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"@vanilla-extract/css": "^1.17.1",
		"@vanilla-extract/css-utils": "^0.1.4",
		"@vanilla-extract/vite-plugin": "5.0.1",
		"commitlint": "^19.8.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.2.1",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"prettier": "~3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"sharp": "^0.33.5",
		"svelte": "^5.23.1",
		"svelte-check": "^4.1.5",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vanilla-lazyload": "^19.1.3",
		"vite": "^6.2.2",
		"vite-imagetools": "^7.0.5"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/sveltekit-vanilla-extract"
	},
	"bugs": {
		"url": "https://github.com/rodneylab/sveltekit-vanilla-extract/issues"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@sveltejs/kit",
			"esbuild",
			"svelte-preprocess"
		]
	}
}
